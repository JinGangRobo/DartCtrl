{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Flash STM32",
            "type": "process",
            "command": "openocd",
            "args": [
                "-f",
                "interface/cmsis-dap.cfg",
                "-f",
                "target/stm32f4x.cfg",
                "-c",
                "program build/Release/dart-ctrl.elf verify reset exit"
            ],
            "dependsOn": ["CMake: build"],
            "options": {
                "cwd": "${workspaceRoot}"
            },
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": ["$gcc"]
        },
        {
            "type": "cmake",
            "label": "CMake: build",
            "command": "build",
            "targets": ["all"],
            "group": "build",
            "problemMatcher": [],
            "detail": "CMake template build task"
        }
    ]
}
